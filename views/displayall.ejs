<%- include('./partials/header') %>

    <table>
        <tr>
            <th>
                First Name
            </th>
            <th>
                Last Name
            </th>
            <th>
                Username
            </th>
            <th>
                Email
            </th>
        </tr>
            
        <%for(let i=0;i<records.length;i++) {%>
            <tr>      

                <td>
                    <%=records[i].fname%></td>    
                </td>

                <td>
                    <%=records[i].lname%></td>    
                </td>
                    
                <td>
                    <%=records[i].username%></td>    
                </td>
                    
                <td>
                    <%=records[i].email%></td>    
                </td>

            </tr>
        <%}%>

    </table>
    
<%- include('./partials/footer') %>